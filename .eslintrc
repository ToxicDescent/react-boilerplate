{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "indent": [2],
    "no-param-reassign": [1, { "props": false }],
    "react/jsx-closing-bracket-location": [1, 'after-props'],
    "new-cap": [0],
    "react/prefer-stateless-function": 0
  }
}
